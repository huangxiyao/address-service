          <tagName>data-match-playbook-${RELEASE_VERSION}</tagName>
          <tagMessage>data match playbook ${RELEASE_VERSION}</tagMessage>
          <createTag>true</createTag>
          <updateTag>false</updateTag>
        </hudson.plugins.git.GitPublisher_-TagToPush>
