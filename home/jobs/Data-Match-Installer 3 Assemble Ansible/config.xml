  <builders>
    <hudson.tasks.Shell>
      <command>cd addressdoctor-infrastructure

[[ -n &quot;${DATA_MATCH_RELEASE_VERSION}&quot; ]] &amp;&amp; sed -Ei -e &quot;s|(^\s*data_match_release_version:\s*).*|\1${DATA_MATCH_RELEASE_VERSION}|&quot; data-match.yml</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>git commit --allow-empty -a -m &quot;Playbook build ${RELEASE_VERSION}&quot;</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
